<p>
	<b class="note-danger-text">Requested products will take 5-7 business days</b>
</p>
<%= simple_form_for(@request_product) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :product_name %>
    <%= f.input :link %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
